{
  "name": "andresceballosm",
  "description": "Andres Felipe Ceballos landing page in TypeScript/Next.js!",
  "version": "1.0.0",
  "private": true,
  "author": "Andres Felipe Ceballos",
  "license": "MIT",
  "keywords": [
    "andresceballos"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
